{
	"servers": {
		"chrome-devtools": {
			"command": "npx",
			"args": [
				"chrome-devtools-mcp@latest"
			]
		},
		"github": {
			"type": "http",
			"url": "https://api.githubcopilot.com/mcp/",
			"headers": {
				"X-MCP-Readonly": "true"
			}
		},
		"playwright": {
			"command": "npx",
			"args": [
				"@playwright/mcp@latest"
			]
		}
	},
	"inputs": []
}