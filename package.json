{
  "name": "canzeltly",
  "version": "1.0.0",
  "description": "A project template for the Canzeltly stack",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "run-p watch serve:client",
    "build": "rollup -c",
    "serve": "run-p watch serve:client",
    "watch": "rollup -c --watch",
    "serve:client": "serve ./dist --single",
    "lint": "prettier src --check && eslint src",
    "fix": "prettier src --write && eslint src --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/megazear7/canzeltly.git"
  },
  "keywords": [
    "Zod",
    "Express",
    "Lit",
    "TypeScript",
    "Project",
    "Template"
  ],
  "author": "Alex Lockhart",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/megazear7/canzeltly/issues"
  },
  "homepage": "https://github.com/megazear7/canzeltly#readme",
  "dependencies": {
    "@lit/context": "^1.1.6",
    "commander": "^14.0.2",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "lit": "^3.3.1",
    "multer": "^2.0.2",
    "openai": "^6.8.1",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-watch-globs": "^2.0.1",
    "serve": "^14.2.5",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.0",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-replace": "^6.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/express": "^5.0.5",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.9.2",
    "eslint": "^9.39.0",
    "nodemon": "^3.1.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "rollup": "^4.52.5",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2"
  }
}
